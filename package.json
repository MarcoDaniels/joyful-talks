{
  "name": "joyful-talks",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "sync": "ts-node utils/sync.ts",
    "start:assets": "COCKPIT_MODE=start ts-node utils/serve.ts",
    "start:elm": "elm-pages develop",
    "start:preview:elm": "elm-live src/Preview.elm --port=3000 --start-page=preview.html -- --output=dist/preview.js",
    "start:preview": "npm-run-all --parallel start:preview:elm start:assets",
    "start": "npm-run-all --parallel start:elm start:assets",
    "serve": "COCKPIT_MODE=serve ts-node utils/serve.ts",
    "build": "elm-pages build",
    "build:style": "node-sass style/index.scss dist/style.css",
    "deploy:build": "ts-node utils/deploy.ts",
    "deploy": "yarn sync && yarn build && yarn deploy:build",
    "clean": "rm -rf dist",
    "test": "elm-test",
    "postinstall": "elm-tooling install"
  },
  "dependencies": {
    "cockpit-http-client": "1.0.1",
    "elm-pages": "1.5.5"
  },
  "devDependencies": {
    "@types/express": "4.17.11",
    "@types/http-proxy": "1.17.5",
    "@types/parseurl": "1.3.1",
    "aws-cli-js": "2.2.0",
    "elm-format": "0.8.4",
    "elm-live": "4.0.2",
    "elm-test": "0.19.1-revision6",
    "elm-tooling": "1.1.0",
    "express": "4.17.1",
    "got": "11.8.2",
    "http-proxy-middleware": "1.0.6",
    "node-sass": "5.0.0",
    "npm-run-all": "4.1.5",
    "ts-node": "9.1.1",
    "typescript": "4.1.5"
  }
}
